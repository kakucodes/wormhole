      "type": "object",
      "required": [
        "all_channel_chains"
      ],
      "properties": {
        "all_channel_chains": {
          "type": "object",
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "channel_chain"
      ],
      "properties": {
        "channel_chain": {
          "type": "object",
          "required": [
            "channel_id"
          ],
          "properties": {
            "channel_id": {
              "$ref": "#/definitions/Binary"
            }
          },
          "additionalProperties": false
